    if (text_size.x < line_width)
        text_size.x = line_width;

    if (out_offset)
        *out_offset = ImVec2(line_width, text_size.y + line_height);  

    if (line_width > 0 || text_size.y == 0.0f)                        
        text_size.y += line_height;

    if (remaining)
        *remaining = s;

    return text_size;
}


