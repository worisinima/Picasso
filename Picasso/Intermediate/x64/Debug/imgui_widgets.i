    (void)( (!!(ImIsPowerOfTwo(flags & ImGuiColorEditFlags__PickerMask))) || (_wassert(L"ImIsPowerOfTwo(flags & ImGuiColorEditFlags__PickerMask)", L"C:\\Users\\yivanli\\Desktop\\Picasso\\Picasso\\Source\\ImGUI\\imgui_widgets.cpp", (unsigned)(4886)), 0) ); 
